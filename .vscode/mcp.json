{
    "servers": {
        "context7": {
            "command": "npx",
            "args": [
                "-y",
                "@upstash/context7-mcp"
            ],
        },
        "shadcn": {
            "command": "npx",
            "args": [
                "shadcn@latest",
                "mcp"
            ],
        },
        "better-auth": {
            "url": "https://mcp.chonkie.ai/better-auth/better-auth-builder/mcp",
        },
        "eslint": {
			"command": "npx",
			"args": ["@eslint/mcp@latest"],
			"env": {}
		}
    }
}